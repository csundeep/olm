	</wsdl:message>

	<wsdl:message name="getUserResponse">
		<wsdl:part name="getUserResponse" element="tns:addUserResponse"></wsdl:part>
	</wsdl:message>

	<wsdl:portType name="UserManagerPortType">

		<wsdl:operation name="addUser">
