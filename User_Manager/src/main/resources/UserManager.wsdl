			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="UserManagerService">
		<wsdl:port name="UserManagerPortType" binding="tns:UserManagerBinding">
			<soap:address
				location="http://localhost:8080/User_Manager-0.0.1-SNAPSHOT/user" />
		</wsdl:port>
	</wsdl:service>

</wsdl:definitions>
